{
  "name": "ben_frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && yarn build:css",
    "start": "next start",
    "lint": "next lint",
    "build:css": "postcss styles/tailwind.css -o styles/globals.css",
    "watch:css": "postcss styles/tailwind.css -o styles/globals.css -w",
    "purge:css": "cross-env NODE_ENV=production yarn run build:css",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check '**/*.{ts,tsx,js,jsx}' --ignore-path .prettierignore --no-error-on-unmatched-pattern",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "format": "prettier --write '**/*.{ts,tsx,js}'",
    "test-all": "yarn run check-types && yarn run check-lint && yarn run build",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "cross-env": "^7.0.3",
    "graphql": "^16.2.0",
    "netlify-plugin-contextual-env": "^0.3.0",
    "next": "12.0.7",
    "next-apollo": "^5.0.7",
    "next-build-id": "^3.0.0",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-back-to-top-button": "^0.0.14",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-otp-input": "^2.4.0",
    "react-phone-input-2": "^2.14.0",
    "react-spinners": "^0.11.0",
    "react-toastify": "^8.1.0",
    "swiper": "^8.0.3",
    "tailwindcss-ripple": "^0.7.1",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.3",
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/node": "^17.0.8",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.38",
    "@types/react-flags-select": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "autoprefixer": "^10.4.2",
    "eslint": "8.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.12",
    "typescript": "^4.5.4"
  }
}
